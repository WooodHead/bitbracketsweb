{ 
  "env": {
    "development": {
       "presets": ["next/babel"]
    },
    "production": {
       "presets": ["next/babel"]
    },
    "test": {
         "presets": [["next/babel", { "preset-env": { "modules": "commonjs" } }]]
    }
  },
  "plugins": [

    ["transform-define", "./env.config.js"],
    ["react-intl", { "messagesDir": "lang/.messages/" }]
  ]
}
